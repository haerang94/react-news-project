age((page) => page + 5);\r\n  };\r\n  // 누른 페이지로 이동\r\n  const onCurrentPage = useCallback(\r\n    (idx) => {\r\n      onPageMove(keyword, idx);\r\n      setCurrentPage((currentPage) => idx);\r\n    },\r\n    [onPageMove, keyword]\r\n  );\r\n\r\n  return (\r\n    <Container>\r\n      <Left onClick={onClickLeft} />\r\n      {Array.from(Array(5), (_, idx) => {\r\n        // 데이터 끝에 도달했을 때\r\n        if (page + idx <= 16 && page + idx <= totalPage)\r\n          return (\r\n            <NewButton\r\n              key={`page-btn-${idx}`}\r\n              onClick={() => onCurrentPage(page + idx)}\r\n            >\r\n              {page + idx}\r\n            </NewButton>\r\n          );\r\n      })}\r\n      <Right onClick={onClickRight} />\r\n    </Container>\r\n  );\r\n});\r\n\r\nexport default Page;\r\n","C:\\Users\\gofkd\\Desktop\\news-project\\news-project\\src\\containers\\FilterContainer.tsx",["122"],"import React, { useState, useCallback, ChangeEvent } from \"react\";\r\nimport Filter from \"components/Filter\";\r\nimport useNews from \"customHooks/useNews\";\r\nimport { Article } from \"types/article\";\r\n//  정렬 컨테이너\r\nconst FilterContainer = React.memo(() => {\r\n  const [sort, setSort] = useState<string>(\"\");\r\n  const { data, onSetNews } = useNews();\r\n  //  날짜 최근 순 정렬\r\n  const date_descending = (a: Article, b: Article) => {\r\n    const x = new Date(a.publishedAt).getTime();\r\n    const y = new Date(b.publishedAt).getTime();\r\n    return x < y ? 1 : -1;\r\n  };\r\n  //  출처 초성 순 정렬\r\n  const sortBySource = useCallback((a: Article, b: Article) => {\r\n    return a.source.name > b.source.name ? 1 : -1;\r\n  }, []);\r\n\r\n  //  정렬 select 태그 내용 변경 & 실제 데이터 정렬\r\n  const onChange = useCallback(\r\n    (e: ChangeEvent<HTMLSelectElement>) => {\r\n      const { value } = e.target;\r\n      setSort(value);\r\n      if (data && value === \"date\") {\r\n        data.sort(date_descending);\r\n        onSetNews(data);\r\n      }\r\n      if (data && value === \"s